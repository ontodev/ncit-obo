obo:IAO_0000115
  rdf:type owl:AnnotationProperty ;
  rdfs:label "definition" ;
  obo:IAO_0000115 "The official definition, explaining the meaning of a class or property. Shall be Aristotelian, formalized and normalized. Can be augmented with colloquial definitions." ;
