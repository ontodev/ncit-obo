obo:IAO_0000117
  rdf:type owl:AnnotationProperty ;
  rdfs:label "term editor" ;
  obo:IAO_0000115 "Name of editor entering the term in the file. The term editor is a point of contact for information regarding the term. The term editor may be, but is not always, the author of the definition, which may have been worked upon by several people" ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

obo:IAO_0000119
  rdf:type owl:AnnotationProperty ;
  rdfs:label "definition source" ;
  obo:IAO_0000115 "formal citation, e.g. identifier in external database to indicate / attribute source(s) for the definition. Free text indicate / attribute source(s) for the definition. EXAMPLE: Author Name, URI, MeSH Term C04, PUBMED ID, Wiki uri on 31.01.2007" ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .
